{
  "mcpServers": {
    "filesystem": {
      "type": "builtin",
      "name": "fs",
      "options": {
        "allowed_directories": [
          "/tmp",
          "/private/tmp"
        ]
      },
      "allowedTools": [
        "read_file",
        "write_file",
        "list_directory"
      ]
    },
    "oracle-oci-compute-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-compute-mcp-server"
      ]
    },
    "oracle-oci-compute-instance-agent-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-compute-instance-agent-mcp-server"
      ]
    },
    "orac-oci-networking-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-networking-mcp-server"
      ],
      "excludedTools": [
        "terminate_instance"
      ]
    },
    "oracle-oci-resource-search-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-resource-search-mcp-server"
      ]
    },
    "oracle-oci-registry-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-registry-mcp-server"
      ]
    },
    "oracle-oci-monitoring-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-monitoring-mcp-server"
      ]
    },
    "oracle-oci-identity-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-identity-mcp-server"
      ]
    },
    "datetime-helper-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.datetime-helper-mcp-server"
      ]
    },
    "oracle-oci-migration-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-migration-mcp-server"
      ]
    },
    "oracle-oci-network-load-balancer-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-network-load-balancer-mcp-server"
      ]
    },
    "oracle-oci-api-mcp-server": {
      "type": "local",
      "command": "uv",
      "args": [
        "run",
        "oracle.oci-api-mcp-server"
      ]
    }
  }
}
